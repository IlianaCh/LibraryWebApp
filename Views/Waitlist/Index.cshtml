@model IEnumerable<LibraryWebApp.Models.waitlist>
@{
    ViewBag.Title = "Index";
}

<div class="title-container">
    <h2 class="title">Waitlist</h2>
</div>

<table class="table">
    <tr>
        <th>
            Item ID
        </th>
        <th>
            List ID
        </th>
        <th>
            Object ID
        </th>
        <th>
            User ID
        </th>
        <th>
            Date Added
        </th>
        <th>

        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr class="table_row">
            <td>
                @Html.DisplayFor(modelItem => item.list_id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.list_item_id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.object_id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.user_id)
            </td>

            <td>
                @Html.DisplayFor(modelItem => item.create_date)
            </td>
            <td>
               
                    @*@Html.ActionLink("Edit ", "Edit", new { id = item.movie_id }, new { @class = "underline" })*@
                    <a href="@Url.Action("Edit", new { id = item.list_id })" class="button-link-edit">Edit</a>
                    @*  @Html.ActionLink("Delete ", "Delete", new { id = item.movie_id }, new { @class = "underline" }) *@
                    <a href="@Url.Action("Delete", new { id = item.list_id })" class="button-link-delete">Delete</a>
            </td>

        </tr>
    }

</table>
