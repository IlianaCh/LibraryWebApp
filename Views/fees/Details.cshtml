@model LibraryWebApp.Models.fee

@{
    ViewBag.Title = "Details";
}

<h2>Details</h2>

<div>
    <h4>Fee</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Date:
        </dt>

        <dd>
            @Html.DisplayFor(model => model.create_date)
        </dd>

        <dt>
            Total Fee:
        </dt>

        <dd>
            @Html.DisplayFor(model => model.total_fee)
        </dd>

        <dt>
            Remaining Fee:
        </dt>

        <dd>
            @Html.DisplayFor(model => model.remaining_fee)
        </dd>

        <dt>
            Paid Status:
        </dt>

        <dd>
            @Html.DisplayFor(model => model.is_paid)
        </dd>

        <dt>
            Transaction #:
        </dt>

        <dd>
            @Html.DisplayFor(model => model.checkout_records.transaction_id)
        </dd>

        <dt>
            Username:
        </dt>

        <dd>
            @Html.DisplayFor(model => model.user.username)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Edit", "Edit", new { id = Model.fee_id }) |
    @Html.ActionLink("Back to List", "Index")
</p>
